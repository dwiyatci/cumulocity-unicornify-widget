{
  "name": "cumulocity-unicornify-widget",
  "version": "1.0.0",
  "description": "a little esoteric Cumulocity widget that involves unicorns and high-dose of happiness",
  "keywords": [
    "cumulocity",
    "ui",
    "plugin",
    "widget",
    "typescript",
    "esoteric",
    "unicorn",
    "happiness"
  ],
  "homepage": "https://github.com/dwiyatci/cumulocity-unicornify-widget",
  "bugs": "https://github.com/dwiyatci/cumulocity-unicornify-widget/issues",
  "license": "MIT",
  "author": "Glenn Dwiyatcita <glenn.dwiyatcita@cumulocity.com> (https://tiny.cc/dwiyatci)",
  "repository": {
    "type": "git",
    "url": "https://github.com/dwiyatci/cumulocity-unicornify-widget.git"
  },
  "scripts": {
    "preinstall:clean": "node ./scripts/preinstall.js rimraf yarn",
    "install:clean": "yarn upgrade || yarn install",
    "update": "yarn upgrade-interactive --latest",
    "start": "yarn tsc:w & yarn plugins:watch & c8y server -u https://developer.cumulocity.com -k -t unicorn",
    "build": "yarn clean && c8y build:plugin unicornify-widget",
    "clean": "rimraf build",
    "lint": "eslint --cache --",
    "test": "karma start --",
    "tsc:w": "tsc -w",
    "plugins:watch": "node ./scripts/plugins-watch.js"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "cornified": "1.0.3",
    "cumulocity-ui-full": "http://resources.cumulocity.com/webapps/ui/full-8.15.6.tar.gz",
    "harlemshakify": "1.0.0",
    "mousetrap": "1.6.1",
    "the-answer": "1.0.0"
  },
  "devDependencies": {
    "@types/angular": "1.6.40",
    "@types/angular-animate": "1.5.9",
    "@types/angular-cookies": "1.4.5",
    "@types/angular-dynamic-locale": "0.1.34",
    "@types/angular-gettext": "2.1.32",
    "@types/angular-mocks": "1.5.11",
    "@types/angular-resource": "1.5.14",
    "@types/angular-route": "1.3.4",
    "@types/angular-sanitize": "1.3.7",
    "@types/angular-ui-bootstrap": "0.13.46",
    "@types/cometd": "2.5.29",
    "@types/d3": "4.12.0",
    "@types/gridstack": "0.0.39",
    "@types/jasmine": "2.8.3",
    "@types/jquery": "3.2.17",
    "@types/jqueryui": "1.12.0",
    "@types/leaflet": "1.2.5",
    "@types/leaflet-label": "0.2.34",
    "@types/lodash": "4.14.92",
    "@types/moment": "2.13.0",
    "@types/ng-file-upload": "12.2.2",
    "@types/payment": "2.1.2",
    "@types/selectize": "0.12.33",
    "@types/three": "0.89.1",
    "@types/ui-select": "0.13.29",
    "angular-mocks": "1.6.8",
    "babel-core": "6.26.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "cumulocity-tools": "2.1.2",
    "eslint": "4.15.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.8.0",
    "fs-extra": "5.0.0",
    "glob": "7.1.2",
    "jasmine-core": "2.8.0",
    "karma": "2.0.0",
    "karma-babel-preprocessor": "7.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "1.1.1",
    "karma-ng-html2js-preprocessor": "1.0.0",
    "karma-spec-reporter": "0.0.32",
    "lodash": "4.17.4",
    "parcel-bundler": "1.4.1",
    "prompt": "1.0.0",
    "tslint": "5.8.0",
